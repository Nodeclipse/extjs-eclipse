<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

    <!-- inference extjs class -->
    <extension
        point="org.eclipse.wst.jsdt.core.inferrenceSupport"
        name="ExtJS Inference Support">
        <inferenceProvider class="net.w3des.extjs.core.infer.InferProvider" />
    </extension> 
    
    <extension 
        point="org.eclipse.wst.jsdt.core.JsGlobalScopeContainerInitializer"
        id="net.w3des.extjs.core.container.point">
        <JsGlobalScopeContainerInitializer
            class="net.w3des.extjs.core.container.ContainerInitializer"
            id="net.w3des.extjs.core.container" />
    </extension>
</plugin>